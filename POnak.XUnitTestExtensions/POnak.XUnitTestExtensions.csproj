<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <EnableNETAnalzers>true</EnableNETAnalzers>
  </PropertyGroup>

  <PropertyGroup>
    <Author>Przemys≈Çaw Onak</Author>
    <Product>POnak.XUnitTestExtensions</Product>
    <Description>Contains test case generators for testing using boundary analysis</Description>
    <PackageTags>xUnit test testing boundary analysis two point three point</PackageTags>
    <Version>0.0.1</Version>
    <PackageId>POnak.XUnitTestExtensions</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/ShadowDancer/POnak.XUnitTestExtensions</PackageProjectUrl>
